{
  "scripts": {
    "build-deps": "cd node_modules/enzyme-adapter-preact/src && babel -d ../../../dist/enzyme-adapter-preact  *.js",
    "build-preact": "npm --prefix ../ run build",
    "pre": "npm run build-preact && mkdir -p dist/components dist/tests",
    "build": "npm run pre && babel -d dist components/*.js tests/*.js",
    "test": "npm run build && node dist/tests/index.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-preact": "^0.1.0",
    "global-jsdom": "^4.2.0",
    "jsdom": "^11.6.2",
    "preact": "^8.2.1",
    "preact-compat": "^3.18.0",
    "preact-compat-enzyme": "^0.2.5",
    "preact-test-utils": "^0.1.3",
    "preact-transition-group": "^1.1.1",
    "react-dom": "^16.2.0",
    "tape": "^4.8.0"
  },
  "license": "AGPL-3.0",
  "readmeFilename": "README.md",
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0"
  }
}
